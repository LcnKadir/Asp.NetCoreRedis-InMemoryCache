@model HashSet<string>

@{
    ViewData["Title"] = "Index";
}
<a asp-action="DeleteFirstList">Baştan Sil</a>

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit">Kaydet</button>
</form>
<br />

@foreach (var item in Model)
{
    <h1>@item<a  asp-action="Delete" asp-route-name="@item">Sil</a></h1>
}

